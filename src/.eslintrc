{
  "parser": "babel-eslint",
  "root": true,
  "extends": [
    "@jetbrains",
    "@jetbrains/eslint-config/es6",
    "@jetbrains/eslint-config/browser",
    "@jetbrains/eslint-config/react",
    "@jetbrains/eslint-config/test"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": [2, 500, 4],
    "complexity": ["error", { "max": 1000 }]
  }
}
